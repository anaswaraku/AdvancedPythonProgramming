    "#create a generator that returns a sequence of numbers from x to y\n",
    "def seq(x,y):\n",
    "    a=min(x,y)\n",
    "    b=max(x,y)\n",
    "    print(\"Sequence\")\n",
    "    while a<=b:\n",
    "        yield a\n",
    "        a+=1\n",
    "        \n",
    "print(\"Input two numbers: \")\n",
    "x=int(input())\n",
    "y=int(input())\n",
    "\n",
    "for a in seq(x,y):\n",
    "    print(a)"
   ]
  }
