   "source": [
    "#to find bigger number among two numbers using lambda\n",
    "a=int(input(\"Enter first number: \"))\n",
    "b=int(input(\"Enter second number: \"))\n",
    "greatest=lambda x,y:max(x,y)\n",
    "result=greatest(a,b)\n",
    "print(\"Greatest among \",a,\" and \",b,\" is \",result)"
   ]
